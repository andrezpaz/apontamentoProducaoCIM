<!DOCTYPE html>
<html lang="pt-BR">
<head>
  <meta charset="UTF-8">
  <title>Dashboard com Filtro Autom√°tico</title>
</head>
<body>
  <iframe id="dashboard-frame" src="http://autoflex.bazei.com.br:7000/auto-login/fi3oo8knu5" width="50%" height="600" frameborder="0"></iframe>

  <script>
    const iframe = document.getElementById('dashboard-frame');
  
    iframe.addEventListener('load', () => {
      const iframeDoc = iframe.contentDocument || iframe.contentWindow.document;
  
      const interval = setInterval(() => {
        const grupoId = '02-IMPRESSORAS (1)';
        const maquinaId = '02-IMPRESSORAS-0001-0208';
  
        const grupoCheckbox = iframeDoc.getElementById(grupoId);
        const maquinaCheckbox = iframeDoc.getElementById(maquinaId);
  
        if (grupoCheckbox && !grupoCheckbox.checked) {
          grupoCheckbox.click(); // Expande o grupo (marca o checkbox do grupo)
        }
  
        if (maquinaCheckbox) {
          maquinaCheckbox.checked = true;
          maquinaCheckbox.dispatchEvent(new Event('change', { bubbles: true }));
          clearInterval(interval);
        }
      }, 300);
    });
  </script>
  
</body>
</html>
